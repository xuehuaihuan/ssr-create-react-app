{
  "name": "ssr-create-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "file-loader": "^5.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts start",
    "client:build": "cross-env REACT_APP_IS_SSR='true' rescripts build",
    "client:start": "cross-env REACT_APP_IS_SSR='true' rescripts start",
    "server:build:dev": "node ./server-webpack/build-dev.js",
    "server:start:dev": "nodemon --watch build --exec node ./build-server-dev/server.js",
    "server:build:prd": "node ./server-webpack/build-prd.js",
    "server:start:prd": "node ./build/server.js",
    "build:ssr": "npm run client:build && npm run server:build:prd",
    "start:ssr": "npm-run-all client:build --parallel server:build:dev server:start:dev",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "standard-react",
      "standard"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@rescripts/cli": "0.0.13",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-react-app": "^9.0.2",
    "cross-env": "^6.0.3",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "standardx": "^5.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "url-loader": "^3.0.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "rescripts": [
    "config-overrides.js"
  ]
}
